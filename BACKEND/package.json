{
  "scripts": {
    "server-dev": "npx nodemon api/server/server.ts",
    "client-dev": "npx tsc --watch",
    "prestart": "npx tsc",
    "start": "npx tsx api/server/server.ts",
    "dev": "run-p server-dev client-dev"
  },
  "type": "module",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^24.0.1",
    "nodemon": "^3.1.7",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "nodemonConfig": {
    "watch": ["./"],
    "ext": "ts,tsx",
    "exec": "tsx api/server/server.ts"

  }
}